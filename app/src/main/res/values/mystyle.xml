<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="mypadding">
        <item name="android:paddingLeft">24dp</item>
        <item name="android:paddingRight">24dp</item>
    </style>

    <style name="charge_ll">
        <item name="android:gravity">center</item>
        <item name="android:orientation">vertical</item>
        <item name="android:background">@drawable/tv_bg_shape</item>
        <item name="android:layout_width">77dp</item>
        <item name="android:layout_height">95dp</item>
    </style>

    <style name="charge_tv_doller">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">18sp</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="charge_tv_msg">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:layout_marginTop">5dp</item>
        <item name="android:textSize">14sp</item>
    </style>

    <style name="charge_title_left">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">1dp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@color/item_text_one</item>
        <item name="android:textSize">16sp</item>
    </style>

    <style name="charge_tv_bottom">
        <item name="android:paddingTop">10dp</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:paddingBottom">10dp</item>
        <item name="android:textSize">16sp</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_width">210dp</item>
        <item name="android:layout_height">40dp</item>
    </style>

    <style name="keyboard_linearlayout">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">60dp</item>
        <item name="android:background">?android:attr/selectableItemBackground</item>
        <item name="android:clickable">true</item>
        <item name="android:focusable">true</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_weight">1</item>
        <item name="android:orientation">vertical</item>
    </style>

    <style name="keyboard_text">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_centerInParent">true</item>
        <item name="android:textColor">@color/item_text_two</item>
        <item name="android:textSize">30sp</item>
    </style>

    <style name="keyboard_text_latter">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_centerInParent">true</item>
        <item name="android:textColor">@color/item_text_two</item>
        <item name="android:textSize">12sp</item>
    </style>

    <style name="call_bg_style">
        <item name="android:layout_marginTop">10dp</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">40dp</item>
        <item name="android:paddingLeft">10dp</item>
        <item name="android:paddingRight">10dp</item>
        <item name="android:layout_marginLeft">40dp</item>
        <item name="android:layout_marginRight">40dp</item>
    </style>

    <style name="call_text">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:layout_centerVertical">true</item>
    </style>

    <style name="call_text_right">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:layout_alignParentRight">true</item>
    </style>

    <style name="defaule_button">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">40dp</item>
        <item name="android:layout_marginRight">40dp</item>
        <item name="android:layout_marginTop">8dp</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">18sp</item>
    </style>

    <style name="notification_tv">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">16sp</item>
    </style>

    <style name="error_button">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">40dp</item>
        <item name="android:layout_marginRight">40dp</item>
        <item name="android:layout_marginTop">8dp</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">16sp</item>
    </style>

    <style name="notification_tv_bottom">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:layout_marginLeft">10dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">16sp</item>
    </style>

    <style name="notification_ll">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:gravity">center</item>
        <item name="android:clickable">true</item>
        <item name="android:orientation">horizontal</item>
    </style>

    <style name="cardview">
        <item name="cardBackgroundColor">#FFFFFF</item>
        <item name="cardCornerRadius">5dp</item>
        <item name="cardUseCompatPadding">true</item>
        <item name="cardPreventCornerOverlap">true</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:focusable">true</item>
        <item name="android:clickable">true</item>
        <!-- <item name="android:foreground">?android:attr/selectableItemBackground</item>-->
    </style>

    <style name="line_style">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">0.5dp</item>
        <item name="android:background">@color/line_color</item>
    </style>

    <style name="charge_bottom_btn_style">
        <item name="android:layout_width">120dp</item>
        <item name="android:layout_height">40dp</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">16sp</item>
        <item name="android:gravity">center</item>
        <item name="android:clickable">false</item>
        <item name="android:background">@drawable/btn_green_shape</item>
       <!-- <item name="android:padding">10dp</item>-->
        <item name="android:textStyle">bold</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:layout_alignParentRight">true</item>
    </style>

    <style name="charge_image_style">
        <item name="android:layout_width">35dp</item>
        <item name="android:layout_height">35dp</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:layout_alignParentLeft">true</item>
    </style>

    <style name="charge_rl">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:clickable">true</item>
        <item name="android:paddingTop">8dp</item>
        <item name="android:paddingBottom">8dp</item>
        <item name="android:orientation">horizontal</item>
    </style>

    <style name="charge_bottom_text">
        <item name="android:layout_centerVertical">true</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_marginLeft">20dp</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">@color/item_text_one</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
</resources>